{
  "request": {
    "method": "POST",
    "urlPath": "/minimax/v1/chat/completions",
    "headers": {
      "Authorization": {
        "contains": "minimax-compression"
      }
    },
    "bodyPatterns": [
      {
        "contains": "files[5]{name,size,type}"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": "chatcmpl-compression-enabled",
      "object": "chat.completion",
      "created": 1234567890,
      "model": "MiniMax-M2.1",
      "choices": [
        {
          "index": 0,
          "message": {
            "role": "assistant",
            "content": "I have processed the TOON-compressed tool result."
          },
          "finish_reason": "stop"
        }
      ],
      "usage": {
        "prompt_tokens": 100,
        "completion_tokens": 10,
        "total_tokens": 110
      }
    }
  }
}
